<p id="notice"><%= notice %></p>

<h1>Cohorts Students</h1>

<table>
  <thead>
    <tr>
      <th>Cohort</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cohorts_students.each do |cohorts_student| %>
      <tr>
        <td><%= cohorts_student.cohort_id %></td>
        <td><%= cohorts_student.student_id %></td>
        <td><%= link_to 'Show', cohorts_student %></td>
        <td><%= link_to 'Edit', edit_cohorts_student_path(cohorts_student) %></td>
        <td><%= link_to 'Destroy', cohorts_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cohorts Student', new_cohorts_student_path %>
